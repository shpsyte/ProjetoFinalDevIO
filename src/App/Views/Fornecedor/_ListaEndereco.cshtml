@model App.ViewModels.FornecedorViewModel
 
<table class="table table-hover"> 
    <thead class="thead-dark">
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Endereco.Street)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco.PostCode)
            </th>
            
            <th></th> 
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => modelItem.Endereco.Street)
